{
  "name": "sveltekit-starter",
  "version": "0.1.0",
  "private": true,
  "description": "Internet Computer SvelteKit starter application",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister",
    "SvelteKit"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --fix --ext .ts",
    "lint-svelte": "eslint . --ext .svelte --plugin eslint-plugin-svelte",
    "lint-svelte-fix": "eslint . --fix --ext .svelte --plugin eslint-plugin-svelte",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier-fix": "prettier --write \"src/**/*.ts\"",
    "prettier-svelte": "prettier --check --plugin prettier-plugin-svelte \"src/**/*.svelte\"",
    "prettier-svelte-fix": "prettier --write --plugin prettier-plugin-svelte \"src/**/*.svelte\"",
    "prettier-motoko": "prettier --check --plugin=prettier-plugin-motoko **/*.mo",
    "prettier-motoko-fix": "prettier --write --plugin=prettier-plugin-motoko **/*.mo",
    "prebuild": "npm run generate",
    "generate": "dfx generate && node ./update.types.mjs"
  },
  "devDependencies": {
    "@dfinity/identity-secp256k1": "^1.0.0",
    "@dfinity/ledger": "next",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@rollup/plugin-inject": "^5.0.3",
    "@skeletonlabs/skeleton": "^2.8.0",
    "@skeletonlabs/tw-plugin": "^0.3.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.25.1",
    "@tailwindcss/forms": "^0.5.7",
    "@types/prismjs": "^1.26.3",
    "@types/wicg-file-system-access": "^2023.10.4",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "node-fetch": "^3.3.2",
    "pem-file": "^1.0.1",
    "postcss": "^8.4.35",
    "postcss-load-config": "^5.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-motoko": "^0.8.4",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-svelte": "^3.2.1",
    "sass": "^1.71.1",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "type": "module",
  "dependencies": {
    "@dfinity/agent": "^1.0.0",
    "@dfinity/auth-client": "^1.0.0",
    "@dfinity/identity": "^1.0.0",
    "@dfinity/principal": "^1.0.0",
    "@dfinity/utils": "next",
    "@floating-ui/dom": "^1.6.3",
    "@nostr-dev-kit/ndk": "^2.4.2",
    "@nostr-dev-kit/ndk-cache-dexie": "^2.2.6",
    "buffer": "^6.0.3",
    "dayjs": "^1.11.10",
    "novel-svelte": "^0.1.6",
    "svelte-feathers": "^0.8.4",
    "svelte-loading-spinners": "^0.3.4",
    "svelte-qrcode-image": "^1.0.0-rc.2",
    "tree-model": "^1.0.7"
  }
}
